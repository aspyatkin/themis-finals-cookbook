(0...<%= @processes %>).to_a.each do |num|
  God.watch do |w|
    w.group = 'app'
    w.name = "app-#{num}"
    w.dir = '<%= @basedir %>'
    w.uid = '<%= @user %>'
    w.gid = '<%= @group %>'
    w.log = "<%= @logs_basedir %>/app-#{num}.log"
    w.start = 'bundle exec ruby backend.rb'
    w.keepalive
    w.env = {
      'TEAM_LOGOS_DIR' => '<%= @team_logos_dir %>',
      'LISTEN' => '127.0.0.1',
      'PORT' => "#{<%= @port_range_start %> + num}",
      'LOG_LEVEL' => '<%= @log_level %>',
      'STDOUT_SYNC' => '<%= @stdout_sync %>',
      'RACK_ENV' => '<%= @rack_env %>'
    }
  end
end
